{
  "name": "vibe-map-app",
  "version": "1.0.0",
  "description": "A mobile navigation app that routes by vibe (greenery, quietness, culture, scenery)",
  "private": true,
  "workspaces": [
    "mobile",
    "supabase/functions/*"
  ],
  "scripts": {
    "dev": "npm run dev:mobile",
    "dev:mobile": "cd mobile && npm run start",
    "dev:supabase": "supabase start",
    "build": "npm run build:mobile",
    "build:mobile": "cd mobile && npm run build",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathPattern=src/test/unit",
    "test:integration": "jest --testPathPattern=src/test/integration",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "deploy:functions": "supabase functions deploy",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geonook/Vibe-Map.git"
  },
  "keywords": [
    "navigation",
    "maps",
    "vibe",
    "routing",
    "react-native",
    "expo",
    "supabase",
    "maplibre",
    "postgis",
    "pgvector"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geonook/Vibe-Map/issues"
  },
  "homepage": "https://github.com/geonook/Vibe-Map#readme",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}