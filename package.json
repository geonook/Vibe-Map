{
  "name": "vibenav",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Emotion-aware navigation app that routes you through healing paths",
  "author": "VibeNav Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/geonook/Vibe-Map.git"
  },
  "keywords": [
    "navigation",
    "emotion",
    "mental-health",
    "maps",
    "routing",
    "therapeutic",
    "vibe"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "maplibre-gl": "^4.7.1",
    "deck.gl": "^9.0.31",
    "@deck.gl/mapbox": "^9.0.31",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.59.0",
    "zustand": "^4.5.5",
    "geojson": "^0.5.0",
    "@turf/turf": "^7.1.0",
    "@turf/helpers": "^7.1.0",
    "@turf/distance": "^7.1.0",
    "@turf/bearing": "^7.1.0",
    "@mapbox/polyline": "^1.2.1",
    "howler": "^2.2.4",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/maplibre-gl": "^3.1.0",
    "@types/geojson": "^7946.0.14",
    "@types/howler": "^2.2.11",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.3.2",
    "typescript": "~5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "@vitest/coverage-v8": "^2.1.2",
    "playwright": "^1.47.2",
    "@playwright/test": "^1.47.2",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "postcss": "^8.4.47",
    "autoprefixer": "^10.4.20",
    "vite-plugin-pwa": "^0.20.5",
    "workbox-window": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
